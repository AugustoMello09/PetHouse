pipeline {
    agent any
    stages {
        stage('Build EurekaService') {
            steps {
                dir ('PetHouseBackend/eureka') {
                   bat 'mvn clean package -DskipTests=true'
                }
            }
        }
        stage('Build Img Docker') {
            steps {
                script {
                    dir('PetHouseBackend/eureka') {
                        bat 'docker build -t augustomello09/pethouse-backend-euraka .'
                    }
                }        
            }
        }
    }
}       