pipeline {
    agent any
    stages {
        stage('Build Email') {
            steps {
                dir ('PetHouseBackend/email') {
                   bat 'mvn clean package -DskipTests=true'
                }
            }
        }
        stage('Build Img Docker') {
            steps {
                script {
                    dir('PetHouseBackend/email') {
                        bat 'docker build -t augustomello09/pethouse-backend-email .'
                    }
                }        
            }
        }
    }
} 